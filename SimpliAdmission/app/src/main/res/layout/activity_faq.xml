                        android:text="@string/Q1"
                        android:textSize="20sp" />

                    <TextView
                        android:id="@+id/ans1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:padding="10dp"
                        android:text=""
                        android:textSize="15sp"
                        android:visibility="gone" />
